<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="backgroundPane" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="75.0" prefHeight="75.0" spacing="10.0" styleClass="horizontalMenuHBox">
         <VBox.margin>
            <Insets bottom="20.0" left="30.0" right="30.0" top="30.0" />
         </VBox.margin>
         <children>
            <ImageView fitHeight="25.6" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../icons/electricLilac/heart-outline.png" />
               </image>
            </ImageView>
            <Label styleClass="labelTitle" text="My Movie List" />
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
      <ScrollPane fx:id="scrollPane" fitToWidth="true" hbarPolicy="NEVER" minHeight="300.0" minWidth="500.0" prefWidth="900.0" styleClass="backgroundPane" stylesheets="@../style.css" vbarPolicy="ALWAYS">
      <content>
         <FlowPane fx:id="flowPane" alignment="TOP_CENTER" hgap="30.0" maxHeight="-Infinity" maxWidth="Infinity" minHeight="-Infinity" prefWidth="900.0" styleClass="backgroundPane" stylesheets="@../style.css" vgap="30.0">
            <padding>
               <Insets bottom="20.0" left="30.0" right="30.0" top="10.0" />
            </padding>
            </FlowPane>
      </content>
      <padding>
         <Insets top="15.0" />
      </padding>
      </ScrollPane>
   </children>
</VBox>
